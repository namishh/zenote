{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:scope",
    {
      "identifier": "fs:write-all",
      "allow": [{ "path": "$HOME/*" }]
    }
  ]
}
